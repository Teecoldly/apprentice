<!DOCTYPE html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <title><%- include("title.ejs") %> </title>
  <!-- base:css -->
  <link rel="stylesheet" href="/vendors/mdi/css/materialdesignicons.min.css">
  <link rel="stylesheet" href="/vendors/base/vendor.bundle.base.css">
  <meta name="google-signin-client_id" content="592380546517-dd4894239at1ebbmnibt36h1k2gcbtk8.apps.googleusercontent.com">
  <!-- endinject -->
  <!-- plugin css for this page -->
  <!-- End plugin css for this page -->
  <!-- inject:css -->
  <link rel="stylesheet" href="/css/style.css">
  <!-- endinject -->
  <link rel="shortcut icon" href="/images/favicon.png" />
  <script src="/js/jquery.js"></script>
  <script src="/js/login.js"></script>
  <script src="/fonts/bootstrap-4.0.0/dist/js/bootstrap.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"
    integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous">
  </script>
  <script src="https://apis.google.com/js/platform.js" async defer></script>
  <link href="/fonts/fontawesome-free-5.13.0-web/css/fontawesome.css" rel="stylesheet">
  <link href="/fonts/fontawesome-free-5.13.0-web/css/brands.css" rel="stylesheet">
  <link href="/fonts/fontawesome-free-5.13.0-web/css/solid.css" rel="stylesheet">
  <link rel="stylesheet" href="/fonts/bootstrap-4.0.0/dist/css/bootstrap.min.css">
  <script src="https://cdn.jsdelivr.net/npm/sweetalert2@9"></script>

</head>

<body>
  <div class="container-scroller">
    <div class="container-fluid page-body-wrapper full-page-wrapper">
      <div class="main-panel">
        <div class="content-wrapper d-flex align-items-center auth px-0 text-right">
          <div class="row w-100 mx-0">
            <div class="col-lg-4 mx-auto">

              <div class="auth-form-light  py-5 px-4 px-sm-5">

                <div class="brand-logo">

                  <img style="width: 300px;" src="/images/IT-LOGO.png" alt="SBACNON">
                </div>

                <h4 class="  text-center">Sign in to continue.</h6>

                  <div class="input-group mb-3">
                    <input type="text" class="form-control form-control-lg" id="user_login" placeholder="Username">
                  </div>
                  <div class="input-group mb-3">

                    <input type="password" class="form-control form-control-lg" placeholder="Password"
                      aria-label="Password" id="pass_login" aria-describedby="basic-addon1" required="">
                    <button class="btn bg-warning text-white float-right" data-toggle="modal"
                      data-target="#forget_password" type=" button"><i class="fa fa-lock m-r-5"></i>
                      ลืมรหัสผ่าน</button>
                  </div>
                  <div class="float-left mr-auto pt-2">
                    <a class="btn  btn-primary btn-lg   " href="#" id="login_user">เข้าสู่ระบบ</a>
                  </div>

                  <div class="float-right">
                    <a class="btn   g-signin2 btn-md" data-longtitle="true" data-onsuccess="onSignIn"></a>
                  </div>

                  <br> <br>  <br> 
                  <div class="text-center mt-4 font-weight-light">
                    สร้างบัญชีใหม่ <a href="#" class="text-primary" data-toggle="modal"
                      data-target="#register_form">ที่นี้</a>
                  </div>

              </div>
              
            </div>
          </div>
        </div>
      </div>
      <!-- content-wrapper ends -->
    </div>
    <!-- page-body-wrapper ends -->
  </div>
  <!-- container-scroller -->
  <!-- base:js -->
  <script src="/vendors/base/vendor.bundle.base.js"></script>
  <!-- endinject -->
  <!-- inject:js -->
  <script src="/js/template.js"></script>
  <!-- endinject -->
  <div class="modal fade" id="register_form" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
    aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header text-dark ">
          <h5 class="modal-title" id="exampleModalLabel">สมัครสมาชิก<h5>
              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true" class="text-dark">&times;</span>
              </button>
        </div>
        <div class="modal-body">

          <div class="form-group row">
            <label for="username_reg" class="col-sm-3 col-form-label">ชื่อผู้ใช้</label>
            <div class="col-sm-9">
              <input type="text" class="form-control" id="username_reg" placeholder="ชื่อผู้ใช้">
            </div>
          </div>
          <div class="form-group row">
            <label for="password_reg" class="col-sm-3 col-form-label">รหัสผ่าน</label>
            <div class="col-sm-9">
              <input type="password" class="form-control" id="password_reg" placeholder="รหัสผ่าน">
            </div>
          </div>
          <div class="form-group row">
            <label for="email_reg" class="col-sm-3 col-form-label">อีเมล์</label>
            <div class="col-sm-9">
              <input type="email" class="form-control" id="email_reg" placeholder="อีเมล์">
            </div>
          </div>
          <div class="form-group row">
            <label for="name_reg" class="col-sm-3 col-form-label">ชื่อจริง</label>
            <div class="col-sm-9">
              <input type="text" class="form-control" id="name_reg" placeholder="ชื่อจริง">
            </div>
          </div>
          <div class="form-group row">
            <label for="lastname_reg" class="col-sm-3 col-form-label">นามสกุล</label>
            <div class="col-sm-9">
              <input type="text" class="form-control" id="lastname_reg" placeholder="นามสกุล">
            </div>
          </div>



        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-dismiss="modal" id="close_reg">ปิด</button>
          <button type="button" class="btn btn-primary" id="submit_reg">สมัครสมาชิก</button>
        </div>
      </div>
    </div>
  </div>
</body>



<div class="modal fade" id="forget_password" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
  aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header text-dark ">
        <h5 class="modal-title" id="exampleModalLabel">ลืมรหัสผ่าน<h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true" class="text-dark">&times;</span>
            </button>
      </div>
      <div class="modal-body">

        <div class="form-group row">
          <label for="username_reg" class="col-sm-3 col-form-label">ชื่อผู้ใช้</label>
          <div class="col-sm-9">
            <input type="text" class="form-control" id="username_forget" placeholder="ชื่อผู้ใช้">
          </div>
        </div>
        <div class="form-group row">
          <label for="password_reg" class="col-sm-3 col-form-label">อีเมล์</label>
          <div class="col-sm-9">
            <input type="text" class="form-control" id="Email_forget" placeholder="อีเมล์">
          </div>
        </div>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal" id="close_forget">ปิด</button>
        <button type="button" class="btn btn-primary" id="submit_forget">กู้คืนรหัสผ่าน</button>
      </div>
    </div>
  </div>
</div>
</body>

</html>